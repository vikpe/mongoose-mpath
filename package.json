    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "async": "^3.2.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.0",
    "chai": "^4.3.7",
    "chai-subset": "^1.6.0",
